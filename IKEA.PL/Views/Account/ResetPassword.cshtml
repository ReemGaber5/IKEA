@model ResetPasswordViewModel
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}


<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ResetPassword" method="post">

			<input class="text email" style="margin-bottom:10px;" type="password" asp-for="NewPassword" placeholder="NewPassword" required="">
			<input class="text email" style="margin-bottom:10px;" type="password" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">

			<input type="submit" value="Reset Password">
		</form>
	</div>
</div>
